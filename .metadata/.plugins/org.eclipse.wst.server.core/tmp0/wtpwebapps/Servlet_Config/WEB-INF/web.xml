<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Servlet_Config</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    

  </welcome-file-list>
  
  <context-param>
  <param-name>driver</param-name>
  <param-value>Hello</param-value>
  </context-param>
  
  <servlet>
  <servlet-name>check</servlet-name>
  <servlet-class>ServletFirst</servlet-class>
  <init-param>
  <param-name>email</param-name>
  <param-value>tops@yahoo.com</param-value>
  </init-param>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>check</servlet-name>
  <url-pattern>/ser1</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>check1</servlet-name>
  <servlet-class>Ser2</servlet-class>
  <init-param>
  <param-name>email</param-name>
  <param-value>tops@gmail.com</param-value>
  </init-param>
  </servlet>
  
   <servlet-mapping>
  <servlet-name>check1</servlet-name>
  <url-pattern>/ser2</url-pattern>
  </servlet-mapping>
  
  
</web-app>

